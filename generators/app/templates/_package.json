{
  "name": "<%= name %>",
  "description": "<%= description %>",
  "version": "<%= version %>",
  "author": {
    "name" : "<%= author %>", 
    "email" : "<%= email %>", 
    "url" : "<%= url %>"
  },
  "scripts": {
    "webpack": "webpack",
    "watch": "webpack --watch",
    "server": "webpack-dev-server --content-base app/ --colors --display-error-details --port 3000",
    "docs": "typedoc  --options typedoc.json  app/**/*.js",
    "start": "npm run server",
    "build": "gulp build && NODE_ENV=production webpack -p --config webpack.production.config.js"
  },
  "dependencies": {
    "ionic-framework": "2.0.0-alpha.35",
    "livereload-js": "^2.2.2",
    "moment": "^2.10.6"
  },
  "devDependencies": {
    "autoprefixer-loader": "^3.1.0",
    "awesome-typescript-loader": "0.12.0-rc.2",
    "css-loader": "^0.22.0",
    "del": "^2.1.0",
    "file-loader": "^0.8.4",
    "gulp": "^3.9.0",
    "node-sass": "^3.4.2",
    "sass-loader": "^3.1.1",
    "style-loader": "^0.13.0",
    "typescript": "1.5.3",
    "url-loader": "^0.5.6",
    "webpack": "^1.12.4",
    "webpack-dev-server": "^1.12.1"
  },
  "cordovaPlugins": [
    "cordova-plugin-whitelist",
    "cordova-plugin-device"
  ],
  "cordovaPlatforms": []
}
